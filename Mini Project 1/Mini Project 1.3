    "# Categorize age groups using pandas.cut()\n",
    "age_bins = [17, 30, 40, 50, 60, 100]\n",
    "age_labels = ['17-29', '30-39', '40-49', '50-59', '60+']\n",
    "data['age_group'] = pd.cut(data['age'], bins=age_bins, labels=age_labels, right=False)\n",
    "\n",
    "# Create a figure and axis\n",
    "fig, ax = plt.subplots()\n",
    "\n",
    "data['age_group'] = pd.cut(data['age'], bins=age_bins, labels=age_labels, right=False)\n",
    "\n",
    "# Calculate the proportions of term deposit \"yes\" and \"no\" for each age group\n",
    "age_group_proportions = data.groupby(['age', 'y']).size().unstack(fill_value=0).div(data['age'].value_counts(), axis=0)\n",
